{
  "name": "vtr-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "bun@1.3.6",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "preview": "vite preview",
    "proto:gen": "mkdir -p src/gen && pbjs -t static-module -w es6 --keep-case --no-service -p .. -p ./node_modules/protobufjs -o src/gen/proto.generated.js ../proto/vtr.proto ../proto/google/rpc/status.proto && pbts -o src/gen/proto.generated.d.ts src/gen/proto.generated.js",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "protobufjs": "^7.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "wcwidth": "^1.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@playwright/test": "^1.57.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "protobufjs-cli": "^2.0.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  }
}
